{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "jsx-double", "avoid-escape", "avoid-template"],
    "jsx-no-multiline-js": false,
    "interface-name": false,
    "member-access": [true, "no-public"],
    "jsx-boolean-value": [true, "never"]
  },
  "rulesDirectory": []
}
